{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": false,
      "noImplicitAny": false,
      "noUnusedParameters": false,
      "strictNullChecks": true,
      "esModuleInterop": true,
      "allowUnreachableCode": false,
      "lib": ["ES2021", "dom"],
      "strictFunctionTypes": true,
      "jsx": "react-jsx",
      "moduleResolution": "node",
      "strictPropertyInitialization": false
    }
}